{
    "name": "IbrahimBlog-node",
    "version": "1.0.0",
    "description": "IbrahimBlog on node",
    "main": "app.js",
    "scripts": {
        "mongo:start": "docker run --name Restfull-mongo -p 27017:27017 mongo & sleep 5",
        "start": "nodemon ./app.js",
        "dev": "nodemon ./app.js ",
        "test-postman": "newman run ./tests-api/api-tests.postman.json -e ./tests-api/env-api-tests.postman.json",
        "test": "jest --watch",
        "stop": "lsof -ti :3000 | xargs kill",
        "mongo:stop": "docker stop Restfull-mongo && docker rm Restfull-mongo"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/IbrahimShamma99/blogPost.git"
    },
    "license": "ISC",
    "dependencies": {
        "body-parser": "^1.19.0",
        "cors": "2.7.1",
        "documentation": "^12.1.4",
        "ejs": "2.5.5",
        "errorhandler": "1.4.3",
        "express": "^4.17.1",
        "express-jwt": "6.0.0",
        "express-session": "^1.17.0",
        "jest": "^24.9.0",
        "jsonwebtoken": "7.1.9",
        "method-override": "2.3.10",
        "methods": "1.1.2",
        "mongoose": "5.13.15",
        "mongoose-unique-validator": "1.0.2",
        "morgan": "^1.9.1",
        "passport": "0.3.2",
        "passport-local": "1.0.0",
        "request": "^2.88.0",
        "slug": "0.9.1",
        "underscore": "1.12.1"
    },
    "devDependencies": {
        "newman": "^3.8.2",
        "nodemon": "^1.11.0",
        "supertest": "^4.0.2",
        "supertest-session": "^4.0.0"
    }
}
